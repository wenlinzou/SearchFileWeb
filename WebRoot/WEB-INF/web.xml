<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>	
  <welcome-file-list>
    <welcome-file>/WEB-INF/jsp/index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>SearchServlet</servlet-name>
  	<servlet-class>com.servlet.SearchServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>InputServlet</servlet-name>
    <servlet-class>com.servlet.InputServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PrintServlet</servlet-name>
    <servlet-class>com.servlet.PrintServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PlayServlet</servlet-name>
    <servlet-class>com.servlet.PlayServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RenameServlet</servlet-name>
    <servlet-class>com.servlet.RenameServlet</servlet-class>
  </servlet>





  <servlet-mapping>
    <servlet-name>SearchServlet</servlet-name>
    <url-pattern>/search.action</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>InputServlet</servlet-name>
    <url-pattern>/input.action</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PrintServlet</servlet-name>
    <url-pattern>/print.action</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PlayServlet</servlet-name>
    <url-pattern>/playServlet.action</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RenameServlet</servlet-name>
    <url-pattern>/rename.action</url-pattern>
  </servlet-mapping>
</web-app>
